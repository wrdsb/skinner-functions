{
    "bindings": [
        {
            "name": "queueMessage",
            "type": "queueTrigger",
            "direction": "in",
            "queueName": "enrolment-replace",
            "connection": "wrdsbskinnerts_STORAGE"
        },
        {
            "name": "recordIn",
            "type": "cosmosDB",
            "databaseName": "skinner",
            "collectionName": "enrolments",
            "id": "{id}",
            "connection": "wrdsb-skinner_CosmosDB",
            "direction": "in"
        },
        {
            "name": "recordOut",
            "type": "cosmosDB",
            "databaseName": "skinner",
            "collectionName": "enrolments",
            "createIfNotExists": true,
            "connection": "wrdsb-skinner_CosmosDB",
            "direction": "out"
        },
        {
            "name": "res",
            "type": "http",
            "direction": "out"
        }
    ],
    "disabled": false,
    "scriptFile": "../dist/enrolment-replace/index.js"
}