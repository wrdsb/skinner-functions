{
    "bindings": [
        {
            "name": "req",
            "type": "httpTrigger",
            "direction": "in",
            "authLevel": "function",
            "methods": [
                "post"
            ]
        },
        {
            "name": "inBlob",
            "type": "blob",
            "path": "{in_blob.path}/{in_blob.name}",
            "connection": "wrdsbskinner_STORAGE",
            "direction": "in"
        },
        {
            "name": "outBlob",
            "type": "blob",
            "path": "{out_blob.path}/{out_blob.name}",
            "connection": "wrdsbskinner_STORAGE",
            "direction": "out"
        },
        {
            "name": "res",
            "type": "http",
            "direction": "out"
        }
    ],
    "disabled": false,
    "scriptFile": "../dist/blob-replace-contents/index.js"
}