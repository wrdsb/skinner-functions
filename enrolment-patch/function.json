{
    "bindings": [
        {
            "name": "triggerMessage",
            "type": "queueTrigger",
            "direction": "in",
            "queueName": "enrolment-patch",
            "connection": "wrdsbskinnerts_STORAGE"
        },
        {
            "name": "recordIn",
            "type": "cosmosDB",
            "databaseName": "skinner",
            "collectionName": "enrolments",
            "id": "{id}",
            "connection": "wrdsb-skinner_CosmosDB",
            "direction": "in"
        },
        {
            "name": "recordOut",
            "type": "cosmosDB",
            "databaseName": "skinner",
            "collectionName": "enrolments",
            "createIfNotExists": true,
            "connection": "wrdsb-skinner_CosmosDB",
            "direction": "out"
        },
        {
            "name": "callbackMessage",
            "type": "queue",
            "direction": "out",
            "queueName": "callbacks",
            "connection": "wrdsb-skinner_CosmosDB"
        }
    ],
    "disabled": false,
    "scriptFile": "../dist/enrolment-patch/index.js"
}